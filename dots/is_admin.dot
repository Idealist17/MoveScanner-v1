digraph {
    0 [ label = "[Block 0 - X]\l" shape=box]
    1 [ label = "[Block 1 - X]\l" shape=box]
    2 [ label = "[Block 2 - offset 4..=5]\l  4: label L1\l  5: goto 9\l" shape=box]
    3 [ label = "[Block 3 - offset 6..=8]\l  6: label L0\l  7: $t4 := 0\l  8: abort($t4)\l" shape=box]
    4 [ label = "[Block 4 - offset 0..=3]\l  0: $t1 := move($t0)\l  1: $t2 := 0x123\l  2: $t3 := ==($t1, $t2)\l  3: if ($t3) goto 4 else goto 6\l" shape=box]
    5 [ label = "[Block 5 - offset 9..=10]\l  9: label L2\l 10: return ()\l" shape=box]
    0 -> 4 [ label = "" ]
    2 -> 5 [ label = "" ]
    3 -> 1 [ label = "" ]
    4 -> 2 [ label = "" ]
    4 -> 3 [ label = "" ]
    5 -> 1 [ label = "" ]
}
